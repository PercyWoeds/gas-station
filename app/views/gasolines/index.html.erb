<p id="notice"><%= notice %></p>

<h1>Listing Gasolines</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Class</th>
      <th>Price</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @gasolines.each do |gasoline| %>
      <tr>
        <td><%= gasoline.name %></td>
        <td><%= gasoline.class %></td>
        <td><%= gasoline.price %></td>
        <td><%= link_to 'Show', gasoline %></td>
        <td><%= link_to 'Edit', edit_gasoline_path(gasoline) %></td>
        <td><%= link_to 'Destroy', gasoline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Gasoline', new_gasoline_path %>
